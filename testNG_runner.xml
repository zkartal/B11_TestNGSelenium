<!--How to run all test cases in one class without one-->
<suite name="ETS_B11 Automation" verbose="1">
    <test name="B11 testNG test">
        <classes>
            <class name="com.eurotech.tests.day12_actions_jse_fileUpload.ActionsTest">
                <methods>
                    <exclude name="rightClick"></exclude>
                    <exclude name="dragAndDrop"></exclude>
                </methods>
            </class>
        </classes>
    </test>
</suite>